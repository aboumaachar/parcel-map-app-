# Application Settings
APP_NAME=Parcel Map Viewer
APP_ENV=production
APP_DEBUG=false

# Database Settings
DB_HOST=postgis
DB_PORT=5432
DB_NAME=parcel_db
DB_USER=parcel_user
DB_PASSWORD=change_me_in_production

# GeoServer Settings
GEOSERVER_URL=http://geoserver:8080/geoserver
GEOSERVER_USER=admin
GEOSERVER_PASSWORD=change_me_in_production

# File Upload Settings
UPLOAD_MAX_SIZE=100MB
ALLOWED_EXTENSIONS=shp,dbf,shx,prj,cpg,kmz,kml

# Optional: webhook URL to notify when a KMZ job permanently fails after retries
JOB_ALERT_WEBHOOK=

# Optional: URL for bull-board (dev-only) UI - default: http://localhost:3003/admin/queues
BULL_BOARD_URL=http://localhost:3003/admin/queues

# Security
SESSION_SECRET=change_me_in_production_use_long_random_string

# Frontend
REACT_APP_API_URL=http://localhost:3001/api

# Docker Resource Limits (optional)
# Uncomment and adjust as needed
# POSTGRES_MAX_MEMORY=2G
# GEOSERVER_MAX_MEMORY=2.5G
# BACKEND_MAX_MEMORY=1G
# FRONTEND_MAX_MEMORY=256M
